{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description": "Simple starter package for Redux with React and Babel support",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "clean": "rimraf build",
    "build:rollup": "rollup -c",
    "build:rollup:watch": "wrollup -c rollup.config.js",
    "build": "yarn run clean && yarn run build:rollup",
    "serve": "browser-sync start -c browser-sync.js",
    "start": "cross-env NODE_PATH='src' run-p build:rollup:watch serve",
    "prestart": "yarn run lint",
    "lint": "eslint src",
    "pretest": "yarn run lint",
    "test": "cross-env NODE_PATH='src' babel-node src/test/index.js",
    "watch": "watch 'clear && yarn run test -s' src"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "6.6.1",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.1",
    "babel-plugin-external-helpers": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "browser-sync": "2.18.5",
    "cheerio": "0.22.0",
    "connect-history-api-fallback": "1.3.0",
    "connect-logger": "0.0.1",
    "cross-env": "3.1.4",
    "deep-freeze": "0.0.1",
    "eslint": "3.12.2",
    "eslint-plugin-react": "6.8.0",
    "ignore-styles": "5.0.1",
    "npm-run-all": "4.0.0",
    "postcss": "5.2.8",
    "react-addons-test-utils": "15.4.1",
    "redbox-react": "1.3.3",
    "rimraf": "2.5.4",
    "rollup": "0.40.0",
    "rollup-plugin-babel": "2.7.1",
    "rollup-plugin-commonjs": "7.0.0",
    "rollup-plugin-node-resolve": "2.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-sass": "0.4.8",
    "rollup-plugin-uglify": "1.0.1",
    "tape": "4.6.3",
    "watch": "1.0.1",
    "wrollup": "0.1.21"
  },
  "dependencies": {
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-redux": "4.4.6",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-promise": "0.5.3"
  }
}
